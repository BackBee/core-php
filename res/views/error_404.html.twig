{% extends 'error_default.html.twig' %}

{% set errors = this.getPageErrors('404') %}

{% block title %}404 Error - {{ this.getApplication().getSite().getLabel() }}{% endblock %}
{% block error_title %}{{ errors.title ?: parent() }}{% endblock %}
{% block error_message %}{{ errors.description ?: parent() }}{% endblock %}
{% block button_title %}{{ errors.button_title ?: parent() }}{% endblock %}
