{% if false == hasToken %}
<script type="text/javascript">
    if (window.opener) {
        {% if 200 == status %}
            window.opener.postMessage({'key':'{{key}}', 'signature':'{{signature}}'}, '*');
        {% else %}
            window.opener.postMessage('{{ message }}', '*');
        {% endif %}
    }

    window.close();
</script>
{% endif %}